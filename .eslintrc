{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "React": "writable"
  },
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": "off"
  },
  "plugins": ["react"]
}
